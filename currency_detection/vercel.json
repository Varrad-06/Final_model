{
    "builds": [
        {
            "src": "currency_detection/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb", "runtime": "python3.11"
            }   
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "currency_detection/wsgi.py"
        }
    ]
}